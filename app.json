{
  "expo": {
    "name": "MumzWorld",
    "slug": "mumzworld",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.yourappname"
    },
    "android": {
      "package": "com.yourcompany.yourappname",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96bd6064-a206-42d6-9d7b-7042fc933695"
      }
    },
    "owner": "luctunechi"
  }
}
